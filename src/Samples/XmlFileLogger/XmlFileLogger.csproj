<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Microsoft.Build.Logging.StructuredLogger</RootNamespace>
    <AssemblyName>XmlFileLogger</AssemblyName>
    <UseProductOutputPath>true</UseProductOutputPath>
    <CopyNuGetImplementations>false</CopyNuGetImplementations>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFrameworks>$(LibraryTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Build.Framework" VersionOverride="14.3.0" />
    <PackageReference Include="Microsoft.Build.Utilities.Core" VersionOverride="14.3.0" />
    <PackageReference Include="Microsoft.Build" VersionOverride="14.3.0" />
  </ItemGroup>
</Project>